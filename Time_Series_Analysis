    "# Questions 3d - Activity 4 - Design Own CSV Parser\n",
    "\n",
    "# Import zip_longest\n",
    "from itertools import zip_longest"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 90,
   "id": "5ab653a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Open sales_record.csv, read first line and use string mehtods to generate a list of all the column name\n",
    "\n",
    "def dict_from_csv(header, line):\n",
    "    zipped_line = zip_longest(header, line, fillvalue=None)\n",
    "    csv_dict = {key_val[0]: key_val[1] for key_val in zipped_line}\n",
    "    return csv_dict\n",
    "\n",
    "with open(\"sales_record.csv\", \"r\") as file:\n",
    "    first_line = file.readline()\n",
    "    header = first_line.replace('\\n', \"\").split(\",\")\n",
    "    for line in enumerate(file):\n",
    "        line = line.replace(\"\\n\", \"\").split(\",\")\n",
    "        d = dict_from_csv(header, line)\n",
    "        print(d)"
   ]
