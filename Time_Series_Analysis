   "source": [
    "# Combine the preceding code into one block\n",
    "\n",
    "def whole_num(list_num):\n",
    "    whole = \" \"\n",
    "    while len(list_num) != 0:\n",
    "        whole += str(list_num.pop())\n",
    "    return int(whole[::-1])\n",
    "\n",
    "for permutation in itertools.permutations(range(3)):\n",
    "    assert isinstance(permutation, tuple)\n",
    "    list_num = list(itertools.dropwhile(lambda x: x <= 0, permutation))\n",
    "    whole = whole_num(list_num)\n",
    "    assert isinstance(whole, int)\n",
    "    print(whole)"
   ]
