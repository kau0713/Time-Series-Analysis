    "# Remove punctuation and special characters\n",
    "cleaned_mul_text = \" \"\n",
    "for char in multiline_text:\n",
    "    if char == \" \":\n",
    "        cleaned_mul_text += char\n",
    "    elif char.isalnum():\n",
    "        cleaned_mul_text += char\n",
    "    else:\n",
    "        cleaned_mul_text += \" \"\n",
    "\n",
    "# Split words\n",
    "list_words = cleaned_mul_text.split()"
   ]
